@model CREA3M.Models.ResponseList<CREA3M.Models.PaymentHistoryModel>

<div class="col-sm-12 m-t-10">
    <table id="myTableHistory" class="table table-striped table-bordered" width="100%">
        <thead>
            <tr>
                <th>FechaCobro</th>
                <th>Nombre</th>
                <th>idFactura</th>
                <th>Folio</th>
                <th>Obervaciones</th>
                <th>Operación</th>
                <th>Numero Cuenta</th>
                <th>Total</th>
                <th>Saldo</th>
                <th>Vendedor</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var elem in Model.model)
            {
                <tr>
                    <td>@elem.FechaCobro</td>
                    <td>@elem.Nombre</td>
                    <td>@elem.idFactura</td>
                    <td>@elem.Folio</td>
                    <td>@elem.Observaciones</td>
                    <td>@elem.Operacion</td>
                    <td>@elem.NumeroCuenta</td>
                    <td>@string.Format("${0:n}", elem.Total)</td>
                    <td>@string.Format("${0:n}", elem.Saldo)</td>
                    <td>@elem.Vendedor</td>
                </tr>
            }
        </tbody>
    </table>
</div>
